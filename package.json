{
  "name": "@bridge/batMap",
  "version": "2.2.1",
  "description": "Unique interface above multiple map providers",
  "publishConfig": {
    "registry": "https://nexus.azure.cicd.solocal.com/repository/leadformance-npm-releases/",
    "@bridge:registry": "https://nexus.azure.cicd.solocal.com/repository/leadformance-npm-releases/"
  },
  "engines": {
    "node": ">=14",
    "yarn": ">=1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Leadformance/batMap.git"
  },
  "author": {
    "name": "Leadformance Developers",
    "email": "dev@leadformance.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "git+https://github.com/Leadformance/batMap.git"
  },
  "homepage": "https://leadformance.github.io/batMap/",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "prepare": "husky install && yarn build",
    "build": "rollup -c",
    "build:dev": "yarn build --watch",
    "cover": "open coverage/lcov-report/index.html",
    "ci-validate": "yarn lint && yarn test:cov",
    "lint": "yarn lint:commit && yarn lint:eslint && yarn lint:prettier",
    "lint:commit": "commitlint --from=$( git rev-list --tags --max-count=1 )",
    "lint:eslint": "eslint --cache --color '{src,test}/**/*.ts'",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "lint:prettier": "yarn prettier -l '{src,test}/**/*.{ts,js,md,json}'",
    "lint:prettier:fix": "yarn prettier --write '{src,test}/**/*.{ts,js,md,json}'",
    "lint:fix": "yarn lint:eslint:fix && yarn lint:prettier:fix",
    "start": "parcel serve --port=8888 --public-url='/' --out-dir='./build' example/*.html example/assets/**/*",
    "test": "jest --forceExit --passWithNoTests",
    "test:cov": "yarn test --ci --coverage"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write",
      "yarn eslint"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional",
      "@bridge/commitlint-config"
    ],
    "rules": {
      "scope-enum": [
        2,
        "always",
        [
          "deps",
          "deps-dev",
          "gmaps",
          "mappy",
          "leaflet",
          "utils",
          "example"
        ]
      ]
    }
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@bridge/commitlint-config": "^2.2.6",
    "@bridge/eslint-config": "^5.2.83",
    "@bridge/semantic-release-config": "^3.3.14",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@testing-library/dom": "^7.30.3",
    "@types/googlemaps": "^3.43.3",
    "@types/jest": "^26.0.22",
    "@types/jsdom": "^16.2.9",
    "@types/leaflet": "^1.7.0",
    "@types/leaflet.markercluster": "^1.4.4",
    "@types/node": "^14.14.37",
    "@types/object-assign": "^4.0.30",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "babel-jest": "^26.6.3",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "rollup": "^2.44.0",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "sass": "^1.32.8",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  }
}
