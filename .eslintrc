{
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "jasmine": true,
    "node": true,
    "es6": true,
    "protractor": true,
    "jest/globals": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "plugins": ["prettier", "jest"],
  "rules": {
    "prettier/prettier": "error"
  }
}
