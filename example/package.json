{
  "name": "batmap-example",
  "version": "0.1.0",
  "description": "A simple project using BatMap",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "preinstall": "cd .. && yarn",
    "build": "parcel build --public-url='/' --out-dir='./build' ./src/*.html ./src/assets/**/*",
    "start": "parcel serve --port=8888 --public-url='/' --out-dir='./build' ./src/*.html ./src/assets/**/*"
  },
  "dependencies": {
    "@bridge/batMap": "link:..",
    "typescript": "link:../node_modules/typescript"
  },
  "devDependencies": {
    "@babel/core": "link:../node_modules/@babel/core",
    "parcel-bundler": "^1.12.5"
  }
}
